{
  "name": "react-native-picker-ts",
  "version": "0.2.1",
  "description": "A lightweight wheel picker for React Native written in TypeScript (JS runtime)",
  "author": "Azael Contramaestre",
  "license": "MIT",
  "homepage": "https://github.com/AzaelContramaestre01/react-native-picker-ts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AzaelContramaestre01/react-native-picker-ts.git"
  },
  "bugs": {
    "url": "https://github.com/AzaelContramaestre01/react-native-picker-ts/issues"
  },
  "keywords": [
    "react-native",
    "picker",
    "wheel",
    "datepicker",
    "typescript",
    "reanimated"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "react-native": "dist/index.cjs.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "react-native": "./dist/index.cjs.js",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.esm.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "dev:example": "concurrently \"npm:dev\" \"npm run start --prefix example\"",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "prepublishOnly": "npm run build && npm run typecheck"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.66",
    "@types/react-native": "^0.73.0",
    "eslint": "^8.57.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "react": ">=18.2 <20",
    "react-native": "^0.74.0",
    "react-native-gesture-handler": "^2.12.0",
    "react-native-reanimated": "^3.10.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ]
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
